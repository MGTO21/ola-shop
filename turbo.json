{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "dist/**",
                ".medusa/server/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "seed": {
            "cache": false
        }
    }
}